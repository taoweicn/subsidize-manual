.list-item {
    padding: 25rpx 48rpx;
    margin-bottom: 6rpx;
    background-color: #fff;
    font-size: 36rpx;
}
.search-title {
    font-weight: 700;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 32rpx;
    overflow: hidden;
}
.search-content {
    line-height: 40rpx;
    max-height: 80rpx;
    font-size: 28rpx;
    overflow: hidden;
}
.notfound {
    padding: 40rpx 48rpx 0;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    font-size: 36rpx;
}
.notfoundImg {
    margin-top: 20vh;
    width: 40vh;
    height: 40vh;
}