.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.title {
    margin: 10%;
    font-weight: 600;
}
.list {
    margin: 10rpx 10%;
}
.text {
    margin: 12rpx 0;
    display: block;
}
.contact {
    margin: 60rpx 10% 0;
    font-size: small;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.contact text {
    padding: 8rpx 0;
}
.contact text:nth-child(2) {
    margin-bottom: 30rpx;
}