.header {
  display: flex;
  width: 92%;
  height: 100rpx;
  padding: 0 4%;
  justify-content: space-around;
  align-items: center;
  background-color: #fff;
}
.search-btn {
  width: 76%;
  height: 60rpx;
  padding: 10rpx 25rpx;
  background-color: #e4ecf4;
  border-radius: 18rpx;
}
.search-icon {
  width: 80rpx;
  height: 80rpx;
}
.content {
  width: 100%;
  height: calc(100vh - 100rpx);
  background-color: #f3f5f8;
}
.placeholder1 {
  width: 100%;
  height: 6rpx;
  background-color: #f3f5f8;
}
.placeholder2 {
  width: 100%;
  height: 60rpx;
  background-color: #f3f5f8;
}
.list-item {
  padding: 25rpx 48rpx;
  margin-bottom: 6rpx;
  background-color: #fff;
  font-size: 36rpx;
}
.search-title {
  font-weight: 700;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  overflow: hidden;
}
.search-content {
  line-height: 40rpx;
  max-height: 80rpx;
  font-size: 28rpx;
  overflow: hidden;
}
.notfound {
  padding: 40rpx 48rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 36rpx;
}
.notfoundImg {
  margin-top: 20vh;
  width: 40vh;
  height: 40vh;
}
.mark {
  color: #5192fc;
}
